  <div class="col-sm-10">
    <h2>
      Étude anthropométrique des fentes bilatérales selon Farkas
    </h2>
    <!-- TITLE_ALTERNATE_GRAPHIC -->
    

    <!-- CONTRIBUTION -->
    
    
  <div>
    Lenoir, Clément<span class="text-secondary"> (créateur)</span>&#8239;; Bayet, Bénédicte, promoteur<span class="text-secondary"> (superviseur académique)</span>&#8239;; UCL.. MEDE - Faculté de médecine et de médecine dentaire<span class="text-secondary"> (institution émettrice d'un diplôme)</span>
  </div>

    

    <!-- PUBLICATION ACTIVITY: PUBLICATION -->
    
      
      
        <ul class="list-unstyled mb-0">
          
            <li>Bruxelles : UCL. Faculté de médecine et de médecine dentaire, 2017</li>
          
        </ul>
      
    

    <!-- EXTENT -->
    
      <div>37 p.</div>
    

    <!-- DURATION -->
    

    <!-- EDITION STATEMENT-->
    

    <!-- FREQUENCY -->
    

    <!-- IS PART OF -->
    

    <!-- ISSUED WITH -->
    
      

    

    <!-- PRECEDED BY -->
    
      

    

    <!-- SUCCEEDED BY -->
    
      

    

    <!-- SUPPLEMENT TO -->
    
      

    

